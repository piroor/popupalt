	</RDF:Seq>


	<RDF:Seq about="chrome://navigator/content/navigatorOverlay.xul">
		<RDF:li>chrome://popupalt/content/popupalt.xul</RDF:li>
	</RDF:Seq>
	<RDF:Seq about="chrome://multiviews/content/navigatorOverlay.xul">
		<RDF:li>chrome://popupalt/content/popupalt.xul</RDF:li>
	</RDF:Seq>
	<RDF:Seq about="chrome://browser/content/browser.xul">
