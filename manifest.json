{
  "manifest_version": 2,
  "name": "__MSG_name__",
  "version": "4.0.1",
  "description": "__MSG_description__",
  "icons": {
    "32": "icons/icon.png"
  },
  "default_locale": "en_US",
  "applications": {
    "gecko": {
      "id": "{61FD08D8-A2CB-46c0-B36D-3F531AC53C12}",
      "strict_min_version": "48.0"
    }
  },
  "background": {
    "scripts": [
      "common/Configs.js",
      "common/common.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "common/Configs.js",
        "common/common.js",
        "content_scripts/content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true
  },
  "permissions": [
    "storage"
  ]
}
